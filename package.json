{
    "name": "ngcp-web-tests-e2e",
    "version": "0.0.1",
    "description": "Cypress.io based E2E testing scenarios for testing ngcp-csc-ui and ngcp-admin-ui products",
    "main": "index.js",
    "author": "Sergii Leonenko <sleonenko@sipwise.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "cypress open",
        "start:csc": "cypress open --config-file cypress.tdd.csc.json",
        "start:aui": "cypress open --config-file cypress.tdd.aui.json",
        "docker:rebuild:local": "docker build -f ./t/Dockerfile . -t ngcp-web-tests-e2e-bullseye",
        "docker:run:local": "bash ./bin/run-docker.sh local",
        "docker:run": "bash ./bin/run-docker.sh",
        "ci:csc": "cypress run --config-file cypress.ci.csc.json",
        "ci:aui": "cypress run --config-file cypress.ci.aui.json",
        "tdd:csc": "cypress run --config-file cypress.tdd.csc.json",
        "tdd:aui": "cypress run --config-file cypress.tdd.aui.json",
        "tdd:setup": "cp ./cypress.tdd.csc.template.json ./cypress.tdd.csc.json && cp ./cypress.tdd.aui.template.json ./cypress.tdd.aui.json"
    },
    "dependencies": {
        "@nuintun/qrcode": "^3.0.1",
        "cypress": "8.7.0",
        "jwt-decode": "2.2.0"
    },
    "devDependencies": {
        "eslint": "^7.15.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-cypress": "^2.11.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^5.0.0"
    }
}
